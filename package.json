{
  "name": "@osahan/wordpress-ui",
  "version": "0.2.5",
  "description": "WordPress UI theme for react-jsonschema-form",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/osahan/wordpress-ui.git"
  },
  "bugs": {
    "url": "https://github.com/osahan/wordpress-ui/issues"
  },
  "homepage": "https://github.com/osahan/wordpress-ui#readme",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "style": "@wordpress/components/build-style/style.css",
  "files": [
    "dist",
    "src/styles.css",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "demo": "cd demo && npm install --legacy-peer-deps && npm run dev",
    "demo:build": "cd demo && npm install --legacy-peer-deps && npm run build",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "peerDependencies": {
    "@rjsf/core": "^6.1.2",
    "@rjsf/utils": "^6.1.2",
    "@wordpress/components": "^28.6.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@rjsf/core": "^6.1.2",
    "@rjsf/utils": "^6.1.2",
    "@rjsf/validator-ajv8": "^6.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@wordpress/components": "^28.6.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^5.6.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-testing-library": "^7.16.0",
    "eslint-plugin-unused-imports": "^4.4.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.[jt]sx?$": [
        "ts-jest",
        {
          "tsconfig": {
            "jsx": "react",
            "esModuleInterop": true
          }
        }
      ]
    },
    "testMatch": [
      "**/__tests__/**/*.test.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@rjsf|@x0k)/)"
    ],
    "extensionsToTreatAsEsm": [
      ".ts",
      ".tsx"
    ]
  }
}
